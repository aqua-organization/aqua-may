{
      "$schema": "https://json.schemastore.org/tsconfig",
      "compilerOptions": {
            "strict": true,
            "skipLibCheck": true,
            "noUncheckedIndexedAccess": true,
            "resolveJsonModule": true,
            "esModuleInterop": true,
            "isolatedModules": true,
            "moduleDetection": "force",
            "target": "ES2022",
            "incremental": false
      }
}
